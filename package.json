{
  "name": "geo-knowledge-hub-feed",
  "private": true,
  "version": "0.2.1",
  "author": {
    "name": "GEO Knowledge Hub Team"
  },
  "keywords": [
    "gatsby",
    "feed",
    "cms",
    "blog-like"
  ],
  "scripts": {
    "develop:cms": "yarn --cwd cms develop",
    "develop:feed": "wait-on http://localhost:1337/admin && yarn --cwd feed develop",
    "develop": "cross-env FORCE_COLOR=1 npm-run-all -l -p develop:*"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "wait-on": "5.2.1",
    "cross-env": "7.0.3"
  },
  "engines": {
    "node": ">=16.0.0 <18.0.0"
  }
}
